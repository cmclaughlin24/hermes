{
  "name": "hermes",
  "version": "1.1.1",
  "description": "",
  "author": "Wm. Curtis McLaughlin",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build:notification": "nest build notification",
    "build:distribution": "nest build distribution",
    "build:iam": "nest build iam",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:notification": "nest start notification --watch",
    "start:distribution": "nest start distribution --watch",
    "start:iam": "nest start iam --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "docker compose -f ./docker/docker-compose.e2e.yaml up -d",
    "test:e2e": "jest --config ./apps/notification/test/jest-e2e.json --detectOpenHandles --forceExit",
    "posttest:e2e": "docker compose -f ./docker/docker-compose.e2e.yaml down && docker compose -f ./docker/docker-compose.e2e.yaml rm -f database"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@bull-board/api": "^5.21.3",
    "@bull-board/express": "^5.21.3",
    "@bull-board/nestjs": "^5.21.3",
    "@golevelup/nestjs-rabbitmq": "^5.4.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/bullmq": "^10.2.3",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/graphql": "^12.2.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/sequelize": "^10.0.1",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.49.1",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.52.1",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.52.1",
    "@opentelemetry/resource-detector-docker": "^0.1.2",
    "@opentelemetry/resources": "^1.25.1",
    "@opentelemetry/sdk-metrics": "^1.25.1",
    "@opentelemetry/sdk-node": "^0.52.1",
    "@opentelemetry/sdk-trace-node": "^1.25.1",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.4",
    "axios": "^1.7.3",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.12.2",
    "cache-manager": "^5.7.6",
    "cache-manager-ioredis-yet": "^2.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dataloader": "^2.2.2",
    "flat": "^5.0.2",
    "graphql": "^16.9.0",
    "handlebars": "^4.7.8",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "nestjs-twilio": "^4.4.0",
    "nodemailer": "^6.9.14",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.7",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "twilio": "^5.0.4",
    "typeorm": "^0.3.20",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.2.0",
    "@types/nodemailer": "^6.4.15",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "overrides": {
    "twilio": {
      "axios": "$axios"
    },
    "@nestjs-modules/mailer": {
      "@nest/common": "$@nest/common"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "main.ts",
      ".module.ts",
      ".dto.ts",
      ".entity.ts",
      "./test/",
      "./config/"
    ],
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@hermes/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@hermes/health(|/.*)$": "<rootDir>/libs/health/src/$1",
      "^@hermes/open-telemetry(|/.*)$": "<rootDir>/libs/open-telemetry/src/$1",
      "^@hermes/iam(|/.*)$": "<rootDir>/libs/iam/src/$1"
    }
  }
}
