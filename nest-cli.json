{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/notification/src",
  "monorepo": true,
  "root": "apps/notification",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/notification/tsconfig.app.json",
    "plugins": [
      "@nestjs/swagger/plugin"
    ],
    "assets": [
      "/templates/*"
    ]
  },
  "projects": {
    "notification": {
      "type": "application",
      "root": "apps/notification",
      "entryFile": "main",
      "sourceRoot": "apps/notification/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification/tsconfig.app.json"
      }
    },
    "distribution": {
      "type": "application",
      "root": "apps/distribution",
      "entryFile": "main",
      "sourceRoot": "apps/distribution/src",
      "compilerOptions": {
        "tsConfigPath": "apps/distribution/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "analytics": {
      "type": "application",
      "root": "apps/analytics",
      "entryFile": "main",
      "sourceRoot": "apps/analytics/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analytics/tsconfig.app.json"
      }
    }
  }
}